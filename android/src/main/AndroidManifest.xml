<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.reactnativenavigation">

    <application>

        <activity android:name="com.reactnativenavigation.controllers.NavigationActivity"
            android:theme="@style/NoActionBar"/>
        <activity
            android:name="com.reactnativenavigation.controllers.PortraitNavigationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBar"/>
        <activity
            android:name="com.reactnativenavigation.controllers.LandscapeNavigationActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar"/>

        <activity
